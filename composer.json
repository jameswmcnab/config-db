{
    "name": "jameswmcnab/config-db",
    "description": "Database config loader for Laravel, for simple key=>value configuration.",
    "license": "MIT",
    "authors": [
        {
            "name": "James William McNab",
            "email": "james@jameswmcnab.com",
            "homepage": "http://www.jameswmcnab.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.5.*",
        "illuminate/database": "5.5.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Jameswmcnab\\ConfigDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jameswmcnab\\ConfigDb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Jameswmcnab\\ConfigDb\\ConfigDbServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Jameswmcnab\\ConfigDb\\Facades\\ConfigDb"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
